<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>pesanPelanggan</managed-bean-name>
		<managed-bean-class>org.ifunpas.fo.cs.beans.PesanPelanggan</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>barang</managed-bean-name>
		<managed-bean-class>org.ifunpas.fo.cs.beans.Barang</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>org.ifunpas.fo.cs.beans.Login</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>fasilitas</managed-bean-name>
		<managed-bean-class>org.ifunpas.fo.cs.beans.Fasilitas</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>layanan</managed-bean-name>
		<managed-bean-class>org.ifunpas.fo.cs.beans.Layanan</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>admin.xhtml</display-name>
		<from-view-id>/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/success.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>admin.xhtml</display-name>
		<from-view-id>/admin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>gagal</from-outcome>
			<to-view-id>/gagal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
